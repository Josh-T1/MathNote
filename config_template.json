{  
        "note-path": "",

        "main-template" : "",

        "macros-path": "",

        "macro-names": [],

        "section-names": {"DEFINITION": "defin", "THEOREM": "theo", "DERIVATION": "der", "PROOF": "pf", "COROLLARY": "corollary", "LEMMA": "lemma", "PROPOSITION": "proposition"},

        "logging-config": {
            "version": 1,
            "disable_existing_loggers": false,
            "filters": {},

            "formatters": {
                "simple": {
                    "format": "[%(asctime)s][%(levelname)s][%(name)s] %(message)s",
                    "datefmt": "%Y-%m-%d %H:%M:%S"
                    }
                },
            
            "handlers": {
                "file": {
                    "class": "logging.handlers.RotatingFileHandler",
                    "level": "DEBUG",
                    "formatter": "simple",
                    "filename": "",
                    "maxBytes": 1000000,
                    "backupCount": 2
                    },
                "stdout": {
                    "class": "logging.StreamHandler",
                    "level": "INFO",
                    "formatter": "simple",
                    "stream": "ext://sys.stdout"
                    }
                },

            "loggers": {
                "course": {
                    "level": "DEBUG",
                    "handlers": ["file", "stdout"]
                },
                "flashcard": {
                    "level": "DEBUG",
                    "handlers": ["file", "stdout"]
                },
                "cli": {
                    "level": "DEBUG",
                    "handlers": ["file", "stdout"]
                }
            }
        }

}    

