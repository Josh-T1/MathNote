{
    "version": 1,
    "disable_existing_loggers": false,
    "filters": {},

    "formatters": {
        "simple": {
            "format": "[%(asctime)s][%(levelname)s][%(name)s] %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
            }
        },
    
    "handlers": {
        "file": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "DEBUG",
            "formatter": "simple",
            "filename": "",
            "maxBytes": 1000000,
            "backupCount": 2
            }
        },

    "loggers": {
        "mathnote": {
            "level": "DEBUG",
            "handlers": ["file"]
        }
    }
}
